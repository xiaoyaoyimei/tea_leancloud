/**index.wxss**/
.home{
border-top:1px solid #ddd;
  display: flex;
  justify-content: center;
  flex-wrap: wrap ;

}
.item{
  box-sizing: border-box;
  width:50%;
    text-align: center;
    padding: 5px;

}
.item image{
  max-width: 100%;
}
.t{
  color: #333;
  font-size: 0.8rem
}
.c{
  color: #787878;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 0.7rem
}

.p{
  color:#f00;
}
 
.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}
 

 
.logincontent {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
    font-size: 14px;
}
 
.logincontent text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}
 
.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}
.wxlogin{
  width:80px;
  height: 64px;
}